        rdfs:isDefinedBy    <http://iflastandards.info/ns/isbd/elements/> ;
        rdfs:label          "has note on bibliographic history"@en , "tiene nota de historia bibliográfica"@es ;
        rdfs:subPropertyOf  <http://iflastandards.info/ns/isbd/elements/P1045> ;
        reg:name            "hasNoteOnBibliographicHistory"@en ;
        reg:status          status:1001 ;
        skos:definition     "Relates a resource to a note relating to the edition being described or the bibliographic history of the work."@en , "Relaciona un recurso con una nota relativa a la edición que se describe o a la historia bibliográfica de la obra."@es .

<http://iflastandards.info/ns/isbd/elements/P1053>
        a                 rdf:Property ;
        rdfs:domain       <http://iflastandards.info/ns/isbd/elements/C2001> ;
        rdfs:isDefinedBy  <http://iflastandards.info/ns/isbd/elements/> ;
        rdfs:label        "tiene extensión"@es , "has extent"@en ;
        reg:name          "hasExtent"@en ;
        reg:status        status:1001 ;
        skos:definition   "Relaciona un recurso con los nombres y números de la unidad o unidades físicas que constituyen el recurso."@es , "Relates a resource to the names and numbers of the physical unit or units constituting the resource."@en .

<http://iflastandards.info/ns/isbd/elements/P1032>
        a                   rdf:Property ;
        rdfs:domain         <http://iflastandards.info/ns/isbd/elements/C2001> ;
        rdfs:isDefinedBy    <http://iflastandards.info/ns/isbd/elements/> ;
        rdfs:label          "has resource identifier"@en , "tiene identificador del recurso"@es ;
        reg:hasSubproperty  <http://iflastandards.info/ns/isbd/elements/P1154> , <http://iflastandards.info/ns/isbd/elements/P1075> , <http://iflastandards.info/ns/isbd/elements/P1076> ;
        reg:name            "hasResourceIdentifier"@en ;
        reg:status          status:1001 ;
