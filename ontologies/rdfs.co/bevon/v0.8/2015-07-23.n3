  dcterms:modified "2014-03-01+09:00"^^xsd:date ;
  status:term_status "unstable"@en .

bevon:Shandy a owl:Class ;
  rdfs:subClassOf bevon:Cocktail , pto:Shandy ;
  skos:prefLabel "Shandy"@en , "섄디"@ko ;
  skos:definition """
    A shandy, or shandygaff, is beer mixed with a soft drink, carbonated
    lemonade, ginger beer, ginger ale, or apple juice.
    """@en ;
  rdfs:comment """
    The proportions of the two ingredients are adjusted to taste, usually
