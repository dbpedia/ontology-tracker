	olca:rangeIncludes foaf:Organization , foaf:Project , lode:Event , schema:Place .

:spokenLanguage a owl:ObjectProperty ;
	rdfs:label "spoken language"@en , "langue parlée"@fr ;
	rdfs:comment "A language which is spoken in some place or event"@en , "Une langue parlée dans un lieu ou lors d'un événement"@fr ;
	rdfs:label "lengua hablada"@es ;
	rdfs:comment "Una lengua que se habla en algún lugar o evento"@es ;
	rdfs:range :Lingvo ;
	olca:domainIncludes foaf:Organization , foaf:Project , lode:Event , schema:Place .

:fromCountry a owl:ObjectProperty ;
	rdfs:label "from country"@en , "pays d'origine"@fr ;
	rdfs:comment "Country of origin of the language"@en , "Pays d'origine de la langue"@fr ;
	rdfs:domain :Lingvo ;
	rdfs:range schema:Country .

:mainLanguage a owl:ObjectProperty ;
	rdfs:label "main language"@en , "langue principale"@fr ;
	rdfs:comment "The main language used in the place or language resource "@en , "La langue principale utilisée dans le lieu ou la ressource linguistique"@fr ;
	rdfs:label "lengua principal"@es ;
	rdfs:comment "Una lengua ampliamente hablada en un lugar (país, región, ciudad...)"@es ;
	rdfs:range :Lingvo ;
	olca:domainIncludes schema:Place , :LanguageResource .

:regionalLanguage a owl:ObjectProperty ;
	rdfs:label "regional language"@en , "langue régionale"@fr ;
	rdfs:comment "A language spoken in a region of a country"@en , "Une langue parlée dans une région d'un pays"@fr ;
	rdfs:label "lengua regional"@es ;
	rdfs:comment "Una lengua hablada en una región o un país"@es ;
	rdfs:range :Lingvo ;
	olca:domainIncludes schema:Country .

:minorityLanguage a owl:ObjectProperty ;
	rdfs:label "minority language"@en , "langue minoritaire"@fr ;
	rdfs:comment "A language spoken by a minority in some country, but not necessarily in a specific region"@en , "Une langue parlée par une minorité dans un pays, mais pas nécessairement dans une région spécifique"@fr ;
	rdfs:label "lengua minoritaria"@es ;
	rdfs:comment "Un lengua hablada por una minoría en algún país, pero no necesariamente en una región específica"@es ;
	rdfs:range :Lingvo ;
	olca:domainIncludes schema:Country .

:historicalLanguage a owl:ObjectProperty ;
	rdfs:label "historical language"@en , "langue historique"@fr ;
	rdfs:comment "A language spoken in the past in this place, but not used anymore"@en , "Une langue parlée dans le passé dans ce lieu, mais plus utilisée aujourd'hui"@fr ;
	rdfs:label "lengua histórica"@es ;
	rdfs:comment "Una lengua hablada en el pasado en este lugar, pero ya no se utiliza"@es ;
	rdfs:range :Lingvo ;
	olca:domainIncludes schema:Place .

:Translation a owl:Class ;
	rdfs:label "Translation"@en , "Traduction"@fr , "Traducción"@es ;
	rdfs:comment "Description of a Translation should include original resource, translated resource, original language and target language. It might also include translator(s) and translation date, and relevant other information using properties inherited from the generic Event class"@en , "La description d'une Traduction inclut au minimum la ressource originale, la ressource traduite, la langue d'origine et la langue cible. Elle peut aussi inclure le(s) traducteur(s), la date de traduction, et toute autre information pertinente utilisant les propriétés de la classe générique Evénement"@fr , "La descripción de una Traducción debe incluir la fuente original, la fuente traducida, la lengua  original y la lengua de  destino. También puede incluir  el o los traductor(es) , la fecha de traducción y otra información relevante utilizando propiedades heredadas de la clase generica Event"@es ;
	rdfs:subClassOf lode:Event ;
	olca:expectedProperty dcterms:date ;
	vann:example <http://www.lingvoj.org/translations/w3c-recs.rdf#translation-css1-en-to-bg> .

:originalLanguage a owl:ObjectProperty ;
	rdfs:label "original language"@en , "langue d'origine"@fr ;
	rdfs:comment "The language of the original ressource"@en , "La langue de la ressource originale"@fr ;
	rdfs:label "lengua original"@es ;
	rdfs:comment "La lengua de la fuente original"@es ;
	rdfs:range :Lingvo ;
	rdfs:domain :Translation .

:targetLanguage a owl:ObjectProperty ;
	rdfs:label "target language"@en , "langue cible"@fr ;
	rdfs:comment "The language of the translated ressource"@en , "La langue de la ressource traduite"@fr ;
	rdfs:label "lengua destino"@es ;
	rdfs:comment "La lengua de la fuente traducida"@es ;
	rdfs:range :Lingvo ;
	rdfs:domain :Translation .

:originalResource a owl:ObjectProperty ;
	rdfs:label "original resource"@en , "ressource originale"@fr ;
	rdfs:comment "The ressource which is translated"@en , "La ressource qui est traduite"@fr ;
	rdfs:label "recurso original"@es ;
	rdfs:comment "La fuente que se traduce"@es ;
	rdfs:domain :Translation .

:translatedResource a owl:ObjectProperty ;
	rdfs:label "translated resource"@en , "ressource traduite"@fr ;
	rdfs:comment "The ressource which is the result of the translation"@en , "La ressource qui est le résultat de la traduction"@fr ;
	rdfs:label "recurso traducido"@es ;
	rdfs:comment "La fuente que resulta de la traducción"@es ;
	rdfs:domain :Translation .

:translator a owl:ObjectProperty ;
	rdfs:label "translator"@en , "traducteur"@fr ;
	rdfs:comment "An agent (person, organization or sofware) responsible of the translation"@en , "Un agent (personne, organisation ou logiciel) responsable de la traduction"@fr ;
	rdfs:label "traductor"@es ;
	rdfs:comment "Un agente (persona, organización o software) responsable de la traducción"@es ;
	rdfs:domain :Translation ;
	rdfs:range foaf:Agent .

:interpreter a owl:ObjectProperty ;
	rdfs:label "interpreter"@en , "intérprete"@es , "interprète"@fr , "tolk"@nl ;
	rdfs:comment "An agent (person or software) responsible of a real-time oral translation"@en , "Un agente (persona, organización o software) responsable de la interpretación oral"@es , "Un agent (personne ou logiciel) responsable de la traduction orale en temps réel"@fr , "De agent (persoon of applicatie) verantwoordelijk voor het tolken"@nl ;
	rdfs:subPropertyOf :translator ;
	rdfs:domain :Translation ;
	rdfs:range foaf:Agent .

:LanguageResource a owl:Class ;
	rdfs:label "Language resource"@en , "Ressource linguistique"@fr ;
	rdfs:comment "Dictionaries, thesauri, language courses, translation services"@en , "Dictionnaires, thesaurus, cours de langue, services de traduction"@fr .

:resourceType a owl:ObjectProperty ;
	rdfs:label "resource type"@en , "type de ressource"@fr ;
	rdfs:comment "A concept defining the type of the language resource"@en , "Un concept définissant le type de la ressource linguistique"@fr ;
	rdfs:domain :LanguageResource ;
	rdfs:range skos:Concept .

:supportedLanguage a owl:ObjectProperty ;
	rdfs:label "supported language"@en , "langue supportée"@fr ;
	rdfs:comment "A language supported by the language resource"@en , "Une langue supportée par la ressource linguistique"@fr ;
	rdfs:domain :LanguageResource ;
	rdfs:range :Lingvo .

:supportingResource a owl:ObjectProperty ;
	rdfs:label "language resource"@en , "resource lingusistique"@fr ;
	rdfs:comment "A resource supporting this language"@en , "Une ressource linguistique pour cette langue"@fr ;
	owl:inverseOf :supportedLanguage ;
	rdfs:domain :Lingvo ;
	rdfs:range :LanguageResource .

:Lingvo rdfs:comment "Een taal gebruikt in geschreven of gesproken communicatie tussen mensen of in welke een bron is geschreven of opgenomen. Het is aan te raden instanties te gebruiken gedefinieerd op basis van de ISO 630 codes, zoals in deze voorbeelden"@nl .

:Translation rdfs:label "Vertaling"@nl ;
	rdfs:comment "De beschrijving of een vertaling zou de oorspronkelijke bron, de vertaalde bron, de brontaal en de doeltaal moeten bevatten. Het kan ook de vertaler(s), de vertaaldatum en andere relevante informatie, gebruik makend van de eigenschappen geërfd van de generieke Event klasse, bevatten"@nl .

:advancedReading rdfs:label "goed leesniveau"@nl ;
	rdfs:comment "De persoon kan de taal goed lezen"@nl .

:advancedSpeaking rdfs:label "goed spraakniveau"@nl ;
	rdfs:comment "De persoon kan de taal goed spreken"@nl .

:advancedUnderstanding rdfs:label "goed luisterniveau"@nl ;
	rdfs:comment "De persoon kan de gesproken taal goed begrijpen"@nl .

:advancedWriting rdfs:label "goed schrijfniveau"@nl ;
	rdfs:comment "De persoon kan de taal goed schrijven"@nl .

:basicRreading rdfs:label "basis leesniveau"@nl ;
	rdfs:comment "De persoon kan eenvoudige taal lezen"@nl .

:basicSpeaking rdfs:label "basis spraakniveau"@nl ;
	rdfs:comment "De persoon kan eenvoudige taal spreken"@nl .

:basicUnderstanding rdfs:label "basis luisterniveau"@nl ;
	rdfs:comment "De persoon kan eenvoudige gesproken taal begrijpen"@nl .

:basicWriting rdfs:label "basis schrijfniveau"@nl ;
	rdfs:comment "De persoon kan eenvoudige taal schrijven"@nl .

:communicationLanguage rdfs:label "kommunikation taal"@nl ;
	rdfs:comment "Een taal gebruikt voor externe communicatie"@nl .

:endangeredLanguage rdfs:label "bedreigde taal"@nl ;
	rdfs:comment "De taal is bedreigd met uitsterven op de gegeven datum"@nl .

:expertReading rdfs:label "uitstekend leesniveau"@nl ;
	rdfs:comment "De persoon kan de taal uitstekend lezen"@nl .

:expertSpeaking rdfs:label "uitstekend spraakniveau"@nl ;
	rdfs:comment "De persoon kan de taal uitstekend spreken"@nl .

:expertUnderstanding rdfs:label "uitstekend luisterniveau"@nl ;
	rdfs:comment "De persoon kan de gesproken taal uitstekend begrijpen"@nl .

:expertWriting rdfs:label "uitstekend schrijfniveau"@nl ;
	rdfs:comment "De persoon kan de taal uitstekend schrijven"@nl .

:extinctLanguage rdfs:label "uitgestorven taal"@nl ;
	rdfs:comment "Er zijn geen mensen meer die deze taal spreken"@nl .

:historicalLanguage rdfs:label "historische taal"@nl ;
	rdfs:comment "Een taal die vroeger in dit gebied gesproken werd, maar wordt nu niet meer gebruikt"@nl .

:intermediateReading rdfs:label "gemiddeld leesniveau"@nl ;
	rdfs:comment "De persoon leest de taal op een gemiddeld niveau"@nl .

:intermediateSpeaking rdfs:label "gemiddeld spraakniveau"@nl ;
	rdfs:comment "De persoon spreekt de taal op een gemiddeld niveau"@nl .

:intermediateUnderstanding rdfs:label "voertaal"@nl ;
	rdfs:comment "De persoon begrijpt de gesproken taal op een gemiddeld niveau"@nl .

:intermediateWriting rdfs:label "gemiddeld schrijfniveau"@nl ;
	rdfs:comment "De persoon schrijft de taal op een gemiddeld niveau"@nl .

:livingLanguage rdfs:label "levende taal"@nl ;
	rdfs:comment "De taal wordt door mensen gesproken op het aangegeven moment"@nl .

:mainLanguage rdfs:label "hoofdtaal"@nl ;
	rdfs:comment "De meest gebruikte taal op deze plek (land, regio, stad ...)"@nl .

:minorityLanguage rdfs:label "minderheidstaal"@nl ;
	rdfs:comment "Een taal die door een minderheid van de bevolking van een land gesproken wordt, maar niet noodzakelijkerwijs in een specifieke regio"@nl .

:nativeLanguage rdfs:label "moedertaal"@nl ;
	rdfs:comment "Deze eigenschap is onafhankelijk van het niveau van spreken, lezen of schrijven"@nl .

:notRead rdfs:label "niet lezen"@nl ;
	rdfs:comment "De persoon leest de taal in het geheel niet"@nl .

:notSpoken rdfs:label "niet spreken"@nl ;
	rdfs:comment "De persoon spreekt de taal in het geheel niet"@nl .

:notUnderstood rdfs:label "niet verstaan"@nl ;
	rdfs:comment "De persoon verstaat de taal in het geheel niet"@nl .

:notWritten rdfs:label "niet schrijven"@nl ;
	rdfs:comment "De persoon schrijft de taal in het geheel niet"@nl .

:officialLanguage rdfs:label "officiële taal"@nl ;
	rdfs:comment "De officiële taal van een land, organisatie, prject of evenement"@nl .

:originalLanguage rdfs:label "oorspronkelijke taal"@nl ;
	rdfs:comment "De taal van de de bron"@nl .

:originalResource rdfs:label "oorspronkelijke bron"@nl ;
	rdfs:comment "De vertaalde bron"@nl .

:regionalLanguage rdfs:label "streektaal"@nl ;
	rdfs:comment "Een taal gesproken in bepaalde streek van een land"@nl .

:spokenLanguage rdfs:label "gesproken taal"@nl ;
	rdfs:comment "Een taal gesproken op een bepaalde plek of tijdens een evenement"@nl .

:targetLanguage rdfs:label "doeltaal"@nl ;
	rdfs:comment "De taal waarin de bron vertaald is"@nl .

:translatedResource rdfs:label "vertaalde bron"@nl ;
	rdfs:comment "Het resultaat van de vertaling"@nl .

:translator rdfs:label "vertaler"@nl ;
	rdfs:comment "De agent (persoon, organisatie of applicatie) verantwoordelijk voor de vertaling"@nl .

:workingLanguage rdfs:label "werktaal"@nl ;
	rdfs:comment "De taal gebruikt voor interne communicatie door een organisatie of binnen een project"@nl .

